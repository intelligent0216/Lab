#include <stdio.h>

long get_fibbonachi_via_cycle(int number)
{
  long number1=1,number2=1;
  for (int q = 0; q < number-2; q++)
  {
    number2+=number1;
    number1=number2-number1;
  }
  return number2;
}

int main(void)
{
  int number;
  scanf("%d", &number);
  printf("%ld", get_fibbonachi_via_cycle(number));
  return 0;
}
