#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <math.h>
#define n 10

int main(void) {
  int arr[n];
  int *s_arr = arr;

  srand(time(NULL));
  for (int q = 0; q < 10; q++) 
  {
    arr[q] = rand() % 101;
    printf("%d ", arr[q]);
  }
  printf("\n");

  for (int q = 0; q < n-1; q++)
  {
    for (int w = 0; w < n - q -1; w++)
    {
      if (arr[w] > arr[w+1])
      {
        arr[w]     = arr[w] + arr[w + 1];
        arr[w + 1] = arr[w] - arr[w + 1];
        arr[w]     = arr[w] - arr[w + 1]; 
      }
    }
  }

  for (int q =0; q < n; q++)
  {
    printf("%d ", arr[q]);
  }

  return 0;
}
